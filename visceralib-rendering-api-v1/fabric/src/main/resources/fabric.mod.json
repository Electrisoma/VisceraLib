{
  "schemaVersion": 1,
  "id": "visceralib_rendering_api_v1",
  "version": "${version}",
  "name": "VisceraLib Rendering API (v1)",
  "description": "Contains code to assist with rendering.",
  "authors": [ "${authors}" ],
  "contributors": [ "${contributors}" ],
  "contact": {
    "homepage": "${github}",
    "sources": "${github}",
    "issues": "${github}/issues"
  },
  "license": "${license}",
  "icon": "assets/visceralib_rendering_api_v1/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [ "net.electrisoma.visceralib.impl.rendering.v1.EntrypointFabric" ],
    "client": [ "net.electrisoma.visceralib.impl.rendering.v1.client.ClientEntrypointFabric" ]
  },
  "mixins": [
    "mixins/visceralib_rendering_api_v1-common.mixins.json",
    "mixins/visceralib_rendering_api_v1-fabric.mixins.json",
    "mixins/visceralib_rendering_api_v1-common.client.mixins.json",
    "mixins/visceralib_rendering_api_v1-fabric.client.mixins.json"
  ],
  "depends": {
    "visceralib_core": "*",
    "fabric-api-base": "*",
    "fabricloader": ">=${fabric}",
    "minecraft": "${minecraft}",
    "java": ">=${java}"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent": {
        "id": "visceralib",
        "name": "VisceraLib",
        "description": "${description}",
        "icon": "assets/visceralib_rendering_api_v1/logo.png",
        "badges": [ "library" ]
      },
      "update_checker": true
    }
  }
}